<h1>Countries</h1>

<table  border="1">
    <tr>
        <th>Country Code</th>
        <th>Country Name</th>
        <th>Country Details</th>
        <th>Action</th>
    </tr>
    <% countrys.forEach((country) =>
    { %>
    <tr>
        <td><%= country.co_code %></td>
        <td><%= country.co_name %></td>
        <td><%= country.co_details %></td>
        <td>Update</td>
        <td>Delete</td>
    </tr>
    <% }) %>
</table>

