<h1>Cities</h1>

<table border="1">
    <tr>
        <th>City Code</th>
        <th>Country Code</th>
        <th>City Name</th>
        <th>Population</th>
        <th>By the Sea</th>
        <th>Area (KM)</th>
    </tr>
    <% citys.forEach((city) =>
    { %>
    <tr>
        <td><%= city.cty_code %></td>
        <td><%= city.co_code %></td>
        <td><%= city.cty_name %></td>
        <td><%= city.population %></td>
        <td><%= city.isCoastal %></td>
        <td><%= city.areaKM %></td>
        <td><a href="/allDetails/<%= city.cty_code %>">All Details</a></td>
    </tr>
    <% }) %>
</table>

